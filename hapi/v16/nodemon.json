{
    "restartable": "rs",
    "ignore": [
      ".git",
      ".vscode/settings.json",
      "node_modules/**/node_modules"
    ],
    "verbose": true,
    "execMap": {
      "js": "node --harmony"
    },
    "watch": [
      "config/",
      "models/",
      "plugins/",
      "routes/",
      "utils/",
      "app.js"
    ],
    "env": {
      "NODE_ENV": "development"
    },
    "ext": "js json"
  }